<schemalist>
  <schema id="org.gnome.shell.extensions.gswmii" path="/org/gnome/shell/extensions/gswmii/">
    <key name="auto-tile-window-types" type="as">
      <default>['NORMAL']</default>
      <summary>Auto Tile Window Types</summary>
      <description>
        Automatically tile windows of the specified types at creation.
      </description>
    </key>
    <key name="max-columns" type="u">
      <range min="1" max="5"/>
      <default>2</default>
      <summary>Columns max number</summary>
      <description>
          Max number of the columns
      </description>
    </key>
    <key name="split-ratio" type="d">
      <range min="0.10" max="0.90"/>
      <default>0.50</default>
      <summary>Split Ratio</summary>
      <description>
        The screen ratio for which windows are split.
      </description>
    </key>
    <key name="margins" type="(iiii)">
      <default>(0, 0, 0, 0)</default>
      <summary>Margins</summary>
      <description>
        The margins in pixels to add to the screen's x, y, width and height.
      </description>
    </key>
  </schema>
  <schema id="org.gnome.shell.extensions.gswmii.keybindings" path="/org/gnome/shell/extensions/gswmii/keybindings/">
    <key name="switch-default-layout" type="as">
      <default>['&lt;Mod1&gt;d']</default>
      <summary>Switch Default Layout</summary>
      <description>
        Switch to the Default Layout.
      </description>
    </key>
    <key name="switch-stacked-layout" type="as">
      <default>['&lt;Mod1&gt;s']</default>
      <summary>Switch Stacked Layout</summary>
      <description>
        Switch to the Stacked Layout.
      </description>
    </key>
    <key name="switch-focus-down" type="as">
      <default>['&lt;Mod1&gt;j']</default>
      <summary>Switch focus down</summary>
      <description>Switch focus down</description>
    </key>
    <key name="switch-focus-up" type="as">
      <default>['&lt;Mod1&gt;k']</default>
      <summary>Switch focus up</summary>
      <description>Switch focus up</description>
    </key>
    <key name="switch-focus-right" type="as">
      <default>['&lt;Mod1&gt;l']</default>
      <summary>Switch focus right</summary>
      <description>Switch focus right</description>
    </key>
      <key name="switch-focus-left" type="as">
      <default>['&lt;Mod1&gt;h']</default>
